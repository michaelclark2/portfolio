// schema.prisma

generator cleint {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Project {
    id String @default(cuid()) @id
    title String
    description String
    deployedURL String
    imageURL String
    techStack String
}